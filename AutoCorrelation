import numpy as np
import matplotlib as plt
from IPython import get_python
from matplotlib.mlab import find
from scipy.signal import fftconvolve

Fs = 8000
f = 350
T = 5.0
n = int(T*Fs)
x = np.linspace(0,T,n,endpoint=False)
signal = 0.9 * np.sin(2*np.pi*f*x)

autoCorr = fftconvolve(signal,signal[::-1], mode='full')
autoCorr = autoCorr[int(len(autoCorr/2):]
plt.subplot(3,1,3)
plt.plot(autoCorr)
plt.show()
d = np.diff(autoCorr)
start = find(d>0)[0]
peak = np.argmax[autoCorr[start:]] + start


